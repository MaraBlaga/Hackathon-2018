{
  "manifest_version": 2,

  "name": "Survey For Later",
  "description": "Some useful description",
  "version": "0.1",

  "browser_action": {
    "default_icon": "S-icon.png",
    "default_popup": "surveys.html"
  },
  "permissions": [
    "storage",
    "activeTab",
    "tabs"
  ],
  "icons": {
    "16": "S-icon.png",
    "128": "S-icon.png"
  },
  "content_scripts": [
	{
	    "matches": ["http://*/l.php*", "https://*/l.php*"],
		"css": [],
		"js": ["iframe.js"],
		"all_frames": true
	}
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}