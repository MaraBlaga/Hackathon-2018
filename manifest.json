{
  "manifest_version": 2,

  "name": "Survey For Later",
  "description": "Some useful description",
  "version": "0.1",

  "browser_action": {
	"default_icon": "icon.png",
	"default_popup": "surveys.html"
  },
  "permissions": [
	"storage",
	"activeTab",
	"tabs"
  ],
  "icons": {
	"16": "awesome-16.png",
	"128": "awesome-128.png"
  },
  "background": {
	"persistent": false,
	"scripts": ["surveys.js"]
  },
  "content_scripts": [
	{
	  "matches": ["http://edigitalsurvey.com/l.php*"],
	  "css": [],
	  "js": ["iframe.js"],
	  "all_frames": true
	}
  ]
}